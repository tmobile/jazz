{
  "JOB_BUILD_URL": "{conf-jenkins-host}/job/create-service/buildWithParameters",
  "JOB_TOKEN": "{job_token}",
  "API_TOKEN": "{api_token}",
  "SVC_USER": "{ci_user}",
  "SERVICE_API_URL": "https://test.execute-api.test-region.amazonaws.com",
  "DEFAULT_PROVIDER_TIMEOUT": 30,
  "DEPLOYMENT_TARGETS": {
    "api": [
      "{conf_deployment_targets_api}"
    ],
    "function": [
      "{conf_deployment_targets_function}"
    ],
    "website": [
      "{conf_deployment_targets_website}"
    ],
    "sls-app": [
      "{conf_deployment_targets_sls-app}"
    ]
  },
  "EVENT_SOURCE_NAME": {
    "s3": {
      "minLength": 3,
      "maxLength": 63,
      "regexPattern": "^[a-z0-9-]+$"
    },
    "dynamodb": {
      "minLength": 3,
      "maxLength": 255,
      "regexPattern": "^[a-zA-Z0-9_.-]+$"
    },
    "sqs": {
      "minLength": 1,
      "maxLength": 80,
      "regexPattern": "^[a-zA-Z0-9-_]+$"
    },
    "kinesis": {
      "minLength": 1,
      "maxLength": 128,
      "regexPattern": "^[a-zA-Z0-9_.-]+$"
    }
  },
  "PRIMARY_DEPLOYMENT_ACCOUNT": {
    "accountId": "{conf-account}",
    "region": "{conf-region}",
    "provider": "{conf-provider}"
  }
}
