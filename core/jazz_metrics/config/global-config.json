{
  "REQUIRED_FIELDS": [
    "service",
    "domain",
    "environment",
    "end_time",
    "start_time",
    "interval",
    "statistics"
  ],
  "ASSET_REQUIRED_FIELDS": [
    "type",
    "asset_name",
    "statistics"
  ],
  "STATISTICS_TYPE": [
    "sum",
    "average",
    "maximum",
    "minimum",
    "samplecount",
    "total"
  ],
  "CF_REGION": "{cf-region}",
  "APINAME": {
    "{conf-apikey-dev}": "{conf_stack_prefix}-dev",
    "{conf-apikey-stg}": "{conf_stack_prefix}-stg",
    "{conf-apikey-prod}": "{conf_stack_prefix}-prod"
  },
  "STACK_PREFIX": "{conf_stack_prefix}",
  "APIGEE": {
    "STATISTICS_MAP": {
      "average": "avg",
      "minimum": "min",
      "maximum": "max",
      "sum": "sum"
    },
    "INTERVAL_MAP": {
      "1": "second",
      "60": "minute",
      "3600": "hour",
      "86400": "day",
      "604800": "week",
      "2592000": "month"
    }
  },
  "ASSET_TYPES": [
    "lambda",
    "apigateway",
    "cloudfront",
    "s3",
    "dynamodb",
    "sqs",
    "kinesis_stream",
    "apigee_proxy",
    "dynamodb_stream",
    "storage_account"
  ]
}
