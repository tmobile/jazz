{
  "BASE_API_URL": "https://{conf-apikey}.execute-api.{conf-region}.amazonaws.com/prod",
  "SERVICE_API": "/jazz/services",
  "LOGIN_API": "/jazz/login",
  "SERVICE_USER": "{jazz_admin}",
  "TOKEN_CREDS": "{jazz_admin_creds}",
  "SERVICES_TABLE_NAME": "{inst_stack_prefix}_services_prod",
  "REGION": "{conf-region}",
  "CASBIN": {
    "TYPE": "{casbin_type}",
    "HOST": "{casbin_host}",
    "USER": "{casbin_user}",
    "PASSWORD": "{casbin_password}",
    "PORT": "{casbin_port}",
    "DATABASE": "{casbin_database}",
    "TIMEOUT": "{casbin_timeout}"
  }
}