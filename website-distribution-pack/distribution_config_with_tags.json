{
  "DistributionConfig": {
    "CallerReference": "{conf_stack_prefix}-{env}-static-website-distribution-{service_name}",
    "Aliases": {
      "Quantity": 0
    },
    "Origins": {
      "Quantity": 1,
      "Items": [{
        "Id": "{conf_stack_prefix}-{env}-static-website-origin-{service_name}",
        "DomainName": "{conf_s3bucketname}.s3.amazonaws.com",
        "OriginPath": "/{origin_path}",
        "S3OriginConfig": {
          "OriginAccessIdentity": "{conf_origin_id}"
        }
      }]
    },
    "DefaultRootObject": "index.html",
    "DefaultCacheBehavior": {
      "TargetOriginId": "{conf_stack_prefix}-{env}-static-website-origin-{service_name}",
      "ForwardedValues": {
        "QueryString": true,
        "Cookies": {
          "Forward": "none"
        }
      },
      "TrustedSigners": {
        "Enabled": false,
        "Quantity": 0
      },
      "SmoothStreaming": true,
      "Compress": true,
      "ViewerProtocolPolicy": "allow-all",
      "MinTTL": 3600
    },
    "CacheBehaviors": {
      "Quantity": 0
    },
    "CustomErrorResponses": {
      "Quantity": 1,
      "Items": [{
        "ErrorCode": 404,
        "ResponsePagePath": "/index.html",
        "ResponseCode": "200",
        "ErrorCachingMinTTL": 300
      }]
    },
    "Comment": "",
    "Logging": {
      "Enabled": false,
      "IncludeCookies": true,
      "Bucket": "",
      "Prefix": ""
    },
    "PriceClass": "PriceClass_All",
    "Enabled": true
  },
  "Tags": {
    "Items": [
      {
        "Key": "application",
        "Value": "jazz"
      },
      {
        "Key": "service",
        "Value": "{service_name}"
      },
      {
        "Key": "domain",
        "Value": "{domain_name}"
      },
      {
        "Key": "environment",
        "Value": "{env}"
      },
      {
        "Key": "owner",
        "Value": "{owner}"
      }
    ]
  }
}
